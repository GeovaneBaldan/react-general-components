{
  "name": "general-components",
  "displayName": "React General Components",
  "description": "Aliases for general react components",
  "version": "1.0.0",
  "publisher": "GeovaneBaldan",
  "repository": {
    "type": "git",
    "url": "https://github.com/GeovaneBaldan/react-general-components"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:general-components.createWebComponent",
    "onCommand:general-components.createMobileComponent"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "general-components.createWebComponent",
        "title": "React: Create Web Component"
      },
      {
        "command": "general-components.createMobileComponent",
        "title": "React: Create Mobile Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "general-components.createWebComponent",
          "group": "componentsGroup@1",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "general-components.createMobileComponent",
          "group": "componentsGroup@1",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "publish": "vsce publish --yarn",
    "build": "vsce package --yarn"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/lodash": "^4.14.191",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "prettier": "2.8.3",
    "typescript": "*"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "mkdirp": "^2.1.3"
  }
}
